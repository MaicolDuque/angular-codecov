{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": ["src/**/*.ts"],
  "exclude": [
    "**/main.ts",
    "src/main.ts",
    "src/polyfills.ts",
    "src/test.ts",
    "src/environments/**",
    "./coverage/**",
    "./dist/**",
    "./cypress/**",
    "**/*.spec.ts",
    "**/*.cy.ts"
  ],
  "report-dir": "coverage/e2e-lcov",
  "check-coverage": true,
  "branches": 20,
  "lines": 20,
  "functions": 20,
  "statements": 20
}
